{
  "name": "aws-sdk-config-loader",
  "version": "0.2.0",
  "description": "AWS config file loader for CLI tools",
  "author": "moqada <moqada@gmail.com>",
  "bugs": {
    "url": "https://github.com/moqada/aws-sdk-config-loader/issues"
  },
  "devDependencies": {
    "aws-sdk": "^2.1.50",
    "babel": "^5.8.23",
    "babel-plugin-espower": "^1.0.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.3.1",
    "eslint-config-moqada": "^3.0.0",
    "isparta": "^3.0.4",
    "mocha": "^2.3.0",
    "mocha-lcov-reporter": "0.0.2",
    "power-assert": "^1.0.0",
    "sinon": "^1.16.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/moqada/aws-sdk-config-loader#readme",
  "keywords": [
    "aws",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moqada/aws-sdk-config-loader.git"
  },
  "scripts": {
    "build": "rm -r lib && babel src --out-dir lib",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "publish": "npm run build && npm publish",
    "test": "eslint src && babel-node $(npm bin)/isparta cover --report text --report html --report lcovonly $(npm bin)/_mocha -- --require test/hook test/*spec.js",
    "testing": "mocha --require test/hook --watch test/*spec.js"
  },
  "dependencies": {
    "os-homedir": "^1.0.1"
  }
}
